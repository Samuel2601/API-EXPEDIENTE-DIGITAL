{
  "name": "ExpedienteDigital",
  "version": "1.0.0",
  "description": "Sistema de Expediente Digital para Contratación Pública - GADM Cantón Esmeraldas",
  "main": "app.js",
  "type": "module",
  "imports": {
    "#src/*": "./src/*",
    "#utils/*": "./utils/*",
    "#models/*": "./src/models/*",
    "#routes/*": "./src/routes/*",
    "#middlewares/*": "./src/middlewares/*",
    "#controllers/*": "./src/controllers/*",
    "#helpers/*": "./src/helpers/*",
    "#config/*": "./src/config/*"
  },
  "scripts": {
    "start": "nodemon app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "build": "echo 'No build step required for Node.js'",
    "seed": "node scripts/seedData.js",
    "migrate": "node scripts/migrate.js",
    "rename-files": "node scripts/rename-files.js",
    "rsync:sync": "node scripts/rsync-sync.js",
    "permissions:auto": "node utils/autoguardar_permisos.js",
    "routes:map": "node utils/routeMapper.js"
  },
  "keywords": [
    "expediente-digital",
    "contratacion-publica",
    "LOSNCP",
    "GADM",
    "esmeraldas",
    "sercop",
    "api",
    "nodejs",
    "mongodb",
    "rsync",
    "gobierno-autonomo"
  ],
  "author": "GADM Cantón Esmeraldas - Departamento de TI",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "jwt-simple": "^0.5.6",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "mongoose": "^8.0.0",
    "mongoose-paginate-v2": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "oracledb": "^5.5.0",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.3",
    "rsync": "^0.6.1",
    "sharp": "^0.34.2",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "optionalDependencies": {
    "sharp-linux-arm64": "^0.34.2",
    "sharp-linux-x64": "^0.34.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gadm-esmeraldas/expediente-digital.git"
  },
  "bugs": {
    "url": "https://github.com/gadm-esmeraldas/expediente-digital/issues"
  },
  "homepage": "https://github.com/gadm-esmeraldas/expediente-digital#readme"
}